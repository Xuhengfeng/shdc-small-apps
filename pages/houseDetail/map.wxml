<oheader text="房源位置" isCenter="center" styleNum="3" statusBarHeight="{{statusBarHeight}}"></oheader>
<view style="height:{{statusBarHeight+50}}px"></view>
<view class="map_container">
  <map class="map" 
       id="map" 
       longitude="{{longitude}}" 
       latitude="{{latitude}}" 
       scale="14" 
       show-location="true" 
       markers="{{markers}}"
       covers="{{covers}}"
       bindmarkertap="makertap"></map>
</view>
<view class="place_info">
  <text>{{placeData.title}}</text>
  <text>{{placeData.address}}</text>
  <text>{{placeData.telephone}}</text>
</view>
<cover-view class="footer">
   <cover-view bindtap="navMap">导航</cover-view>
</cover-view> 
<scroll-view scroll-x="{{false}}">

<block wx:for="{{list}}">
  <!-- <navigator url="" > -->
      <view class="item">
            <!-- 删除 -->
            <view data-index="{{index}}"
                bindtap="delItem"
                class="cancel">删除</view>
            <!-- 内容层 -->
            <view class="content"
                data-index="{{index}}"
                bindtouchstart="touchS"
                bindtouchmove="touchM"
                bindtouchend="touchE"
                style="{{item.txtStyle}}">
                <view>
                    <image wx:if="{{false}}"  src="../../images/select-off.png"></image>
                    <image wx:if="{{true}}" src="../../images/select-on.png"></image>
                </view>
                <view>
                    <image src=""></image>
                </view>
                <view>
                    <view class="item-title">{{item.txtStyle}}</view>
                    <view class="description">
                        {{item.areaName}} {{item.districtName}} {{item.buildAge}}建成  {{item.buildType}}
                    </view>
                    <view class="houseLabel">
                        <text wx:for="{{item.houseTag}}" 
                            wx:key="{{index2}}"
                            wx:for-item="item2" 
                            wx:for-index="index2">{{item2}}</text>
                    </view>
                    <view class="housePrice">{{item.rentPrice}}
                        <text>元/月</text>
                    </view>
                </view>
           </view>

          
    </view>
  <!-- </navigator> -->
</block>

</scroll-view>


@import "../template/template.wxss";
page{
  background: #ffffff;
}
::-webkit-scrollbar{
  display: none;
}
navigator{
  overflow: hidden;
}
.header{
  position: relative;
  height: 400rpx;
  background: #f9f9f9;
}
.header swiper{
  height: 100%;
}

.serarch-content{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: transparent;
  padding: 20rpx 0;
  z-index: 1100;
  height: 120rpx;
  box-sizing: border-box;
}
.serarch-content .search-bg{
  margin-left: 50%; 
  transform: translateX(-50%);
  background: rgba(255,255,255,.9);
  border-radius: 15rpx;
  width: 660rpx;
  height: 80rpx;
  line-height: 80rpx;
  box-shadow: 0 0 10rpx rgba(0,0,0,.1); 
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}
.serarch-content .search{
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  width: 100%;
}
.serarch-content .search image{
  width: 25rpx;
  height: 25rpx;
  margin: 10rpx 10rpx 0 30rpx;
}
.serarch-content .search input{
  font-size: 26rpx;
  flex: 1;
}
.navbar {
  margin-top: 30rpx;
}
.navbar .nav-content{
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  text-align: center;
  width: 100%;
  height: 70rpx;
  background: #f9f9f9;  
}
.house-info-item{
  flex: 102rpx 0 0;
  width: 102rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #f9f9f9;
}
.on{
  color: red; 
  border-bottom: 5rpx solid red;
}
/* 列表内容 */
.item .item-title{
  font-size:30rpx;
  line-height: 1.2;
  text-align: justify;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.houseTag text{
  margin-right: 10rpx;
}
.houseTag text:nth-of-type(1){
  background: #edf9ff;
  color: #00a8ff;
}
.houseTag text:nth-of-type(2){
  background: #ebfff3;
  color:#00b969;
}


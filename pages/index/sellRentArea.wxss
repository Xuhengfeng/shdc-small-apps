.search-box{
    position: fixed;
    top: 0;
    width: 100%;
    padding: 40rpx 35rpx 0;
    background: #f9f9f9;
    box-sizing: border-box;
}
.search{
    height: 90rpx;
    display: flex;
    flex-flow: row nowrap;
    background: #ffffff;
    justify-content: center;
    align-items: center;
    text-align: center;
    border-radius: 10rpx;
    overflow: hidden;
}
.search .img{
    flex: 70rpx 0 0;
    width: 70rpx;
    height: 70rpx;
    line-height: 90rpx;
}
image{
    width: 35rpx;
    height: 35rpx;
}
.search{
  margin-bottom: 25rpx;
}
.search input{
    flex: 1;
    text-align: left;
}
.search .confirmBtn{
    flex: 120rpx 0 0;
    width: 120rpx;
    height: 100%;
    line-height: 90rpx;
    background: #e6e6e6;
}
.title{
    height: 100rpx;
    line-height: 100rpx;
    background: #f9f9f9;
    font-size: 30rpx;
    color: #999999;
}
.section{
    padding-top: 250rpx;
    background: #ffffff;
}
.section>view>view{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1px solid #e6e6e6;
    margin: 0 35rpx;
}
.section>view>view>text{
    float: right;
    height: 100rpx;
    line-height: 100rpx;
}
.fontColor{
    color: #ffffff;
    background: #ff4343!important;
}
<!-- 单元号 -->
<view class="search-box">
    <view class="search"> 
      <view class="img" bindtap="startsearch"><image src="../../images/search.png"/></view>
      <input confirm-type="搜索"  bindconfirm="searchSubmit" placeholder="请输入单元号"  bindinput="userSearch" value="{{inputTxt}}" />
      <view class="confirmBtn" >确定</view>
    </view> 
</view>   
<view class="section">
    <view class="title">快速选择</view>
    <view wx:for="{{unit}}" data-item="{{item}}" catchtap="selectItem">
        <view>{{item}}</view>
    </view>
</view>

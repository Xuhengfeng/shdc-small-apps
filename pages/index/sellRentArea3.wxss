@import "../template/template.wxss";
.search-box{
    padding: 40rpx 35rpx 25rpx;
    background: #f9f9f9;
}
.search{
    height: 90rpx;
    display: flex;
    flex-flow: row nowrap;
    background: #ffffff;
    justify-content: center;
    align-items: center;
    text-align: center;
    border-radius: 10rpx;
    overflow: hidden;
}
.search .img{
    flex: 70rpx 0 0;
    width: 70rpx;
    height: 70rpx;
    line-height: 90rpx;
}
image{
    width: 35rpx;
    height: 35rpx;
}
.search input{
    flex: 1;
    text-align: left;
}
.search .confirmBtn{
    flex: 120rpx 0 0;
    width: 120rpx;
    height: 100%;
    line-height: 90rpx;
    background: #e6e6e6;
}
.osection{
    background: #ffffff;
}
.osection>view>view{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1px solid #e6e6e6;
    margin: 0 35rpx;
}
.otitle{
    height: 100rpx;
    line-height: 100rpx;
    background: #f9f9f9;
    padding: 0 35rpx;
    font-size: 30rpx;
    color: #999999;
}
.fontColor{
    color: #ffffff;
    background: #ff4343!important;
}
@import "../template/template.wxss";
page{
  background: #ffffff;
}
::-webkit-scrollbar{
  display: none;
}
navigator{
  overflow: hidden;
}
.headerTab{
  padding-top: 40px!important;
}
.page_hd .flexBox{
  display: flex;
  flex-direction: row;
  flex-flow: nowrap;
  justify-content: center;
  align-items: center;
  text-align: center;
  line-height: 100rpx;
}
.flexBox .line{
  flex: 1;
  height: 1px;
  background: #c7c7c7;
}
.flexBox text{
  flex: 1;
  font-size: 30rpx;
  color: #c7c7c7;
}
button{
  margin-top: 40rpx;
  border-radius: 0;
  font-size: 40rpx;
  color: #ffffff;
  text-align: center;
}
.page_hd{
  padding: 40rpx;
  height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.navbar{
  position: fixed;
  top: 0;
  background: #ffffff;
}
/* 重写weui的tab样式 */
.weui-navbar__item.weui-bar__item_on{
  color: #ff4343;
}
.weui-navbar__slider{
  background-color: #ff4343;
}

/* 房源失效样式 */
.item{
  position: relative;
}
.originHouse,
.lose{
  position: absolute;
  top: 30rpx;
  left: 0;
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 16rpx;
  background: #00b969;
  color: #ffffff;
  font-size: 20rpx;
  border-top-right-radius: 17rpx;
  border-bottom-right-radius: 17rpx;
}
.lose{
  background: #666666;
}
.shadow{
  position: absolute;
  width: 240rpx;
  height: 200rpx;
  top: 30rpx;
  left: 0;
  background: rgba(0,0,0,0.2);
}
.changecolor{
  color: #666666!important;
  text-decoration:line-through;
}
.nodata{
  position: fixed;
  width: 186rpx;
  height: 136rpx;
  text-align: center;
  top: 30%;
  left: 50%;
  transform: translateX(-50%);
}
.nodataContent{
  position: fixed;
  text-align: center;
  top: 50%;
  left: 50%;
  transform: translateX(-50%);
  color: #666666;
}
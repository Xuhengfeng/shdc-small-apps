/* pages/mine/mybuyhouse.wxss */
.item{
  display: flex;
  flex-flow: row nowrap;
  padding: 30rpx;
  background:  #fff;
  margin-top: 20rpx;
  align-items: center;
}
.item>view:nth-of-type(2){
  width: 155rpx;
  white-space: nowrap;
  text-align: center;
  margin-left: 90rpx;
  font-size: 26rpx;
  color: #999999;
}
.item>view:nth-of-type(2)>view:nth-of-type(2){
  margin-top: 20rpx;
}
.item>view:nth-of-type(1){
  flex: 1;
  font-size: 34rpx;
  color: #333333;
  line-height: 1.2;
}
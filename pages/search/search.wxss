.wrapper {
  height: 100vh;
}
/* 关闭滚动条  */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.list-search {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 90rpx;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  z-index: 20;
  background: #FFF;
}
.list-search-box {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  width: 100%;
  height: 70rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  position: relative;
}
.list-search-box .selectItem+image{
  display: inline-block;
  margin: 0 20rpx 0 10rpx; 
  width: 14rpx;
  height: 9rpx;
  vertical-align: middle;
}
.list-search-box input{
  width: 100%;
  border-left: 1px solid #cacaca;
  padding-left: 50rpx;
}
.selectItem{
  font-size: 30rpx;
  color: #333333;
  float: left;
  white-space: nowrap;
}
.list-search-box .options{
  position: absolute;
  left: 0;
  top: 70rpx;
  background: #ffffff;
  box-shadow: 0 0 25rpx 1rpx rgba(0,0,0,.1);
}
.list-search-box .options text{
  display: block;
  padding: 20rpx;
  width: 150rpx;
  box-sizing: border-box;
}
.list-search-box .search{
  position: relative;
}
.list-search-box .search>view{
  position: absolute;
  top: 5px;
  left: 0;
  width: 25rpx;
  height: 25rpx;
}
.list-search-box .search image{
  width: 100%;
  height: 100%; 
  display: inline-block;
  margin: 0 10rpx 0 20rpx; 
  vertical-align: middle;
}
.cancelBtn{
  flex-shrink: 0;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  margin-left: 10rpx;
  background: none;
}
.cancelBtn::after{
  border: 0;
}
.history{
  padding: 150rpx 40rpx 0;
}
.history>text{
  font-size: 40rpx;
  color: #333333;
}
.history .label{
  margin-top: 40rpx;
}
.history .label text{
  display: inline-block;
  padding: 15rpx 20rpx;
  background: #f3f3f3;
  font-size: 30rpx;
  color: #666666;
  margin: 0 10rpx 20rpx 0;
  border-radius: 10rpx;
}
.clear{
  margin-left: 50rpx;
  font-size: 28rpx!important;
}
/* 按钮切换高亮 */
.gray{
  background: #f2f2f2;
}
.white{
  background: #ffffff;
}